<main>

    <section class="eleves">
        <table>
            <caption>
                liste eleves
            </caption>
                <tr>
                  <th scope="col">id</th>
                  <th scope="col">nom</th>
                  <th scope="col">prenom</th>
                  <th scope="col">email</th>
                  <th scope="col">bouton</th>
                </tr>
                    <% resulthat.forEach(element => { %>
                        <tr>
                            <!-- <th scope="row">Donuts</th>
                            <td>3,000</td> -->
                            <td><%= element.id %></td>
                            <td><%= element.nom %></td>
                            <td><%= element.prenom %></td>
                            <td><%= element.email %></td>
                            <td>
                                <form action="/delete" method="post">
                                    <input type="hidden" name="id" value="<%= element.id %>">
                                    <button>supprimer</button>
                                </form>
                                <!-- <a href="/listeEleve/:id">supprimer</a> -->
                            </td>
                        </tr>
                    <% }); %>
        </table>
    </section>
</main>